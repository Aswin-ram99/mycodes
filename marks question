#include <stdio.h>
#include <math.h>

int main(){

    int marks;
    printf("What is your marks (0-100) ");
    scanf("%d" , &marks);
    if ((marks >= 90 && marks<100)){
        printf("A+");
    }
     else if (marks>=70 && marks<90){
        printf("A");
    }
    else if( (marks>=30 && marks<70)){
        printf("B");
    }
    else if ( marks<30){
        printf("F");

    }
    else {
        printf("Invalid score");
    }
    
    return 0;

    }
