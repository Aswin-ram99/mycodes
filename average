#include <stdio.h>
#include <math.h>

int main(){

    int a;
    int b;
    int c;

    printf("first number ");
    scanf("%d" , &a);
    printf("second number ");
    scanf("%d" , &b);
    printf("third number ");
    scanf("%d" , &c);

    printf("your average is %d" , (a+b+c)/3);

    return 0;
}
