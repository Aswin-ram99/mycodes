#include <stdio.h>
int main(){
    int n;
    int count=0;
    int array[23]= {2,4,6,7,5,4,3,1,2,3,5,6,7,8,9,8,7,6,5,4,3,3};
    printf("Number you want to check(1-9) ");
    scanf("%d" , &n);
    for(int i=0; i<=22; i++){
        if (array[i]==n){
            count = count+1;
        }
    }
    printf("%d" , count);
    return 0;
}
    
